{
  "projFile": "./Main.fsproj",
  "outDir": "./dist/fable",
  "sourceMaps": true,
  "module": "es2015",
  "dll": true,
  "coreLib": "fable-core/es2015",
  "targets": {
    "fable": {},
    "es2015": {
      "rollup": {
        "dest": "./dist/es2015/Main.js",
        "format": "es",
        "plugins": [
          ["sourcemaps"],

          ["babel", {
            "include": ["./src/js/**"]
          }]
        ]
      }
    },
    "umd": {
      "rollup": {
        "dest": "./dist/umd/Main.js",
        "format": "umd",
        "plugins": [
          ["sourcemaps"],

          ["babel", {
            "include": ["./src/js/**"]
          }]
        ]
      }
    }
  }
}
