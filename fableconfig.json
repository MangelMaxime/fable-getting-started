{
  "babelrc": true,
  "sourceMaps": true,
  "projFile": "./src/fs/Main.fsproj",
  "outDir": "./dist/fable",
  "dll": true,
  "coreLib": "fable-core/es2015.js",
  "targets": {
    "production": {
      "scripts": {
        "postbuild": "rollup --config"
      }
    },
    "debug": {
